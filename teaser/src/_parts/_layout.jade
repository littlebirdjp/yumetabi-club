//------------------------------------------
//- defalt variables
//------------------------------------------
- var title             = 'yumetabi.club';//-meta title
- var description       = '夢を追う人と、それを応援する人のためのプロジェクト。';//-meta description
- var keywords          = '';//-meta keywords
- var og_url            = 'http://yumetabi.club';//-og url
- var og_type           = 'website';//-og type
- var og_title          = title;//-og title
- var og_image          = 'http://yumetabi.club/img/ogimage.png';//-og image
- var og_description    = description;//-og description
- var bodyClass         = '';//body -class
- var rootPath          = './'//-root path

//- override default variables
block var_block

doctype html
html(lang="ja")
head
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1")
    title #{title}
    meta(name="description", content="#{description}")
    meta(name="keywords", content="#{keywords}")
    meta(name="og:url", content="#{og_url}")
    meta(name="og:type", content="#{og_type}")
    meta(name="og:title", content="#{og_title}")
    meta(name="og:image", content="#{og_image}")
    meta(name="og:description", content="#{og_description}")

    block headCSS
    link(rel="stylesheet", href="#{rootPath}css/style.css")

    script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js")

    block headJS

body(class="#{bodyClass}")

    .wrapper

        block header_block
        include _header

        .content#content
            block contents_block

        block footer_block
        include _footer


