- var title             = 'hatsune.yumetabi.club';//-meta title
- var description       = '';//-meta description
- var keywords          = '';//-meta keywords
- var og_url            = '';//-og url
- var og_type           = 'website';//-og type
- var og_title          = '';//-og title
- var og_image          = '';//-og image
- var og_description    = '';//-og description
- var tw_type           = '';//-twitter card type
- var tw_site           = '';
- var tw_url            = '';
- var tw_title          = '';
- var tw_description    = '';
- var tw_image          = '';
- var tw_domain         = 'yumetabi.club';
- var bodyClass         = '';//-body class
- var rootPath          = './'//-root path

//- override default variables
block var_block

doctype html
html(lang="ja")
    head
        meta(charset="utf-8")
        meta(name="viewport", content="width=device-width, initial-scale=1")
        title #{title}
        meta(name="description", content="#{description}")
        meta(name="keywords", content="#{keywords}")
        meta(name="og:url", content="#{og_url}")
        meta(name="og:type", content="#{og_type}")
        meta(name="og:title", content="#{og_title}")
        meta(name="og:image", content="#{og_image}")
        meta(name="og:description", content="#{og_description}")
        meta(name="twitter:card", content="#{tw_type}")
        meta(name="twitter:site", content="#{tw_site}")
        meta(name="twitter:url", content="#{tw_url}")
        meta(name="twitter:title", content="#{tw_title}")
        meta(name="twitter:description", content="#{tw_description}")
        meta(name="twitter:image", content="#{tw_image}")
        meta(name="twitter:domain", content="#{tw_domain}")

        link(rel="apple-touch-icon", sizes="57x57", href="/apple-touch-icon-57x57.png")
        link(rel="apple-touch-icon", sizes="60x60", href="/apple-touch-icon-60x60.png")
